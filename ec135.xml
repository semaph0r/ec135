  <stall aoa="0" width="0" peak="1.5"/>
</hstab>

<!-- left skid -->
<gear x="-0.884" y="+0.94" z="-1.662" skid="1" compression="0.10" spring="0.8" sfric="0.8" dfric="0.7"/>
<gear x="-3.6" y="+0.94" z="-1.5" skid="1" compression="0.10" spring="0.8" sfric="0.8" dfric="0.7"/>

