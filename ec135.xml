<!-- official RD + 0.53 m = yasim RD (reference datum) -->

<!-- FDM based also on information given in NASA-CR-3144; TR-1087-1-->

<airplane mass="3268">

	<approach speed="48" aoa="16">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="0.3"/>
	</approach>

	<cruise speed="130" alt="0">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="0.5"/>
	</cruise>



	<rotor name="main" x="-2.75" y="0.0" z="1.55" nx="0.05" ny="0" nz="1." fx="1" fy="0" fz="0" ccw="1"
  maxcollective="15.8" mincollective="0.2"
  mincyclicele="-4.7" maxcyclicele="10.5"
  mincyclicail="-4.23" maxcyclicail="5.65"
  diameter="10.2" numblades="4" weightperblade="67" relbladecenter="0.5"
  dynamic="1" rpm="394" rellenflaphinge="0.18" delta3="0"
  delta=".25"
  pitch-a="10"
  pitch-b="15"
  flapmin="-15"
  flapmax="15"
  flap0="-7.5"
  flap0factor="0"
  notorque="0"
  dragfactor="0.30"
  translift-ve="20"
  translift-maxfactor="1.3"
  ground-effect-constant="0.1"
  twist="-10.2"
  taper="1"
  chord="0.27"
  number-of-segments="8"
  number-of-parts="8"
  rel-len-where-incidence-is-measured="0.7"
  rel-len-blade-start="0.076"

  airfoil-lift-coefficient="6.9"
       airfoil-drag-coefficient0="0.0065" 
       airfoil-drag-coefficient1="0.1" 
       incidence-stall-zero-speed="18" 
       incidence-stall-half-sonic-speed="9" 
       lift-factor-stall="0.18" 
       drag-factor-stall="8.0" 
       stall-change-over="5.5" 
	  cyclic-factor="1.0"
       rotor-correction-factor="0.75" 
       downwashfactor ="0.1" 
>
		
		<control-input axis="/controls/flight/aileron-trim" control="CYCLICAIL" split="true"/>
		<control-input axis="/controls/flight/aileron" control="CYCLICAIL"
					   src0="-1.0" src1="1.0"
					   dst0="-1.0" dst1="1.0"/>
		<control-input axis="/controls/flight/elevator-trim" control="CYCLICELE" split="true"/>
		<control-input axis="/controls/flight/elevator" control="CYCLICELE"
					   src0="-1.0" src1="1.0"
					   dst0="-1.0" dst1="1.0"/>
		<control-input axis="/controls/engines/engine[0]/throttle" control="COLLECTIVE"
					   src0="0.0" src1="1.0"
					   dst0="1.0" dst1="-1.0"/>
	</rotor>


	<rotor name="tail" x="-8.85" y="0.08" z="0.687" 
nx="0.07" ny="-1" nz="-0.05" fx="1" fy="0" fz="0" ccw="1"
	  maxcollective="12" mincollective="-7.5"
	  diameter="1" numblades="10" weightperblade="1" relbladecenter="0.7"
	  dynamic="1" rpm="3584" rellenflaphinge="0.0" delta3="0" translift="0" delta="0.5"
	  pitch-a="10"
	  pitch-b="15"
	  airfoil-lift-coefficient="6.4"
	  airfoil-drag-coefficient0="0.0035"
	  airfoil-drag-coefficient1="0.10"
	  notorque="0"
	  taper="1"
	  chord="0.205"
	  number-of-segments="5"
	  number-of-parts="4"
	  rel-len-blade-start="0.4"
	  incidence-stall-zero-speed="16"
	  incidence-stall-half-sonic-speed="18"
	  lift-factor-stall="0.18"
	  stall-change-over="5.5"
	  drag-factor-stall="8"
	  rotor-correction-factor="0.7"
	  downwashfactor="1.0"
>
		
		<control-input axis="/controls/flight/rudder-trim" control="COLLECTIVE" invert="true"/>
  <control-input axis="/controls/flight/rudder" control="COLLECTIVE" invert="true"
                 src0="-1.0" src1="1.0"
                 dst0="-1.0" dst1="1.0"/>
</rotor>


	<rotorgear
	  max-power-engine="840"
	  max-power-rotor-brake="100"
	  rotorgear-friction="0.7"
	  engine-prop-factor="0.005"
	  engine-accel-limit="1.96"
	  yasimdragfactor="30"
	  yasimliftfactor="80">
	  
		
		<control-input axis="/controls/engines/engine[0]/magnetos" control="ROTORGEARENGINEON"/>
		<control-input axis="/controls/rotor/brake" control="ROTORBRAKE"
					   src0="0.0" src1="1.0"
					   dst0="0.0" dst1="1.0"/>
	</rotorgear>


	<cockpit x="-1.88" y=".33" z="0.6"/>

<fuselage ax="0.381" ay="0" az="-.4" bx="-5.3" by="0" bz="-.07" cx="3" 
          width="1.92" taper="0.95" midpoint="0.5" idrag="0"/>

