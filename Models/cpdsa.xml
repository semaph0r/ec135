<animation>
	<type>select</type>
	<object-name>gsdi_face</object-name>
	<condition>
	<not>
	<property>sim/model/hide-gsdi</property>
	</not>
	</condition>
</animation>

<!--Switches and Knobs! Switches and Knobs everywhere- on the panel: animation-->
<animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>HI.incr</object-name>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property></property>
    <step type="double">-0.5</step>
   </binding>          
  </action>
 </animation>

 <animation>
  <type>pick</type>
  <visible>false</visible>
  <object-name>HI.decr</object-name>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property></property>
    <step type="double">0.5</step>
   </binding>          
  </action>
 </animation>
 
