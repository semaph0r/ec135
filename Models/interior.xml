<?xml version="1.0"?>

<PropertyList>

	<path>interior.ac</path>
	<offsets>
		<z-m>0.0</z-m>
		<y-m>0.0</y-m>
		<x-m>0.0</x-m>

		<pitch-deg>0.0</pitch-deg>
		
