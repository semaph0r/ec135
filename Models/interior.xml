<!--Doors-->
<animation>
   <type>pick</type>
   <object-name>frontdoor.L.i</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
        <command>nasal</command>
        <script>ec135.leftFrontDoor.toggle();</script>
     </binding>
   </action>
 </animation>

<animation>
   <type>rotate</type>
   <object-name>frontdoor.L.i</object-name>
   <property>sim/model/ec135/door-positions/leftFrontDoor/position-norm</property><interpolation>
     <entry>
       <ind>0.0</ind>
       <dep>0.0</dep>
     </entry>
     <entry>
       <ind>0.25</ind>
       <dep>0</dep>
     </entry>
     <entry>
       <ind>1</ind>
       <dep>60.0</dep>
     </entry>
   </interpolation>
<center>
			<x-m>0.816197</x-m>
			<y-m>-0.744383</y-m>
			<z-m>0.166909</z-m>
		</center>
		<axis>
			<x>-0.289498</x>
			<y>-0.005941</y>
			<z>-0.503452</z>
		</axis>
 </animation>
 
